import { ADD_PERSON } from '../actions/types';
import PeopleInfo from "../utilities/PeopleInfo"
const initialState = {
  people: PeopleInfo
};
const graphReducer = (state = initialState, action) => {
  switch(action.type) {
    case ADD_PERSON:
      return {
        ...state,
        places: state.people.concat({
          key: Math.random(),
          value: action.payload
        })
      };
    default:
      return state;
  }
}
export default graphReducer;